import java.io.*;
import java.util.*;
public class EvnOdd
{
    public static void main(String args[])
    {
        Scanner scn=new Scanner(System.in);
        int n=scn.nextInt();
        if(n/2==0)
        {
            System.out.println("The entered num is even");
        }
        else
        {
            System.out.println("The entered num is odd");
        }
    }
}
